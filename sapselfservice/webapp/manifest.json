{
  "_version": "1.65.0",
  "sap.app": {
    "id": "sapselfservice",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.15.3",
      "toolsId": "35a7d11c-cef1-4ded-a911-250e5af59785"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.131.1",
      "libs": {
        "sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.m": {},
				"sap.ui.unified": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "sapselfservice.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ],
      "js": [{
        "uri": "backend/firebase.js"
      }]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "sapselfservice.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "login",
          "pattern": ":?query:",
          "target": ["TargetLogin"]
        },
        {
          "name": "Main",
          "pattern": "Main",
          "target": ["TargetMain"]
        },

        {
          "name": "Modules",
          "pattern": "Modules",
          "pattern": "Modules",
          "name": "Modules",
          "pattern": "Modules",
          "name": "Modules",
          "target": "Modules"
        },
        {
          "name": "Module1",
