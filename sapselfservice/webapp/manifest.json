          "pattern": "Module5",
          "name": "Module5",
          "target": "Module5"
        },
        {
          "name": "Module6",
          "pattern": "Module6",
          "pattern": "Module6",
          "name": "Module6",
          "pattern": "Module6",
          "name": "Module6",
          "target": "Module6"
        },
        {
          "name": "Quiz",
          "pattern": "Quiz",
          "pattern": "Quiz",
          "name": "Quiz",
          "pattern": "Quiz",
          "name": "Quiz",
          "target": "Quiz"
        },
        {
          "name": "FailurePage",
          "pattern": "FailurePage",
          "pattern": "FailurePage",
          "name": "FailurePage",
          "pattern": "FailurePage",
          "name": "FailurePage",
          "target": "FailurePage"
        },
        {
          "name": "profile",
          "pattern": "Profile",
          "pattern": "Profile",
          "name": "profile",
          "pattern": "Profile",
          "name": "profile",
          "target": "profile"
        },
        {
          "name": "absenceOverview",
          "pattern": "AbsenceOverview",
          "pattern": "AbsenceOverview",
          "name": "absenceOverview",
          "pattern": "AbsenceOverview",
          "name": "absenceOverview",
          "target": "absenceOverview"
        },
        {
          "name": "newAbsenceRequest",
          "pattern": "NewAbsenceRequest",
          "pattern": "NewAbsenceRequest",
          "name": "newAbsenceRequest",
          "pattern": "NewAbsenceRequest",
          "name": "newAbsenceRequest",
          "target": "newAbsenceRequest"
        },
        {
          "name": "TimeTracking",
