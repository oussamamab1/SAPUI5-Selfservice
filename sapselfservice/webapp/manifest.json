        },
        "AbsenceDetailsManager": {
          "viewName": "AbsenceDetailsManager",
          "viewLevel": 2,
          "viewId": "AbsenceDetailsManager"
        }
      }
    },
    "rootView": {
      "viewName": "sapselfservice.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
